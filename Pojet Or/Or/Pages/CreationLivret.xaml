<PageFunction x:Class="Or.Pages.CreationLivret"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      x:TypeArguments="sys:Int64"        
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Or.Pages"
      xmlns:ec="clr-namespace:Or.Business"         
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CreationLivret">

    <Grid>
        <TextBlock x:Name="titre" HorizontalAlignment="Left" Margin="196,112,0,0" TextWrapping="Wrap" Text="Creation d'un livret associé à la carte" VerticalAlignment="Top" FontSize="28
                   " Background="White"/>
        <Button x:Name="retour" Content="Retour" HorizontalAlignment="Left" Margin="474,240,0,0" VerticalAlignment="Top" FontSize="18" Click="Retour_Click"/>
        <Button x:Name="creer" Content="Ajouter un livret" HorizontalAlignment="Left" Margin="231,240,0,0" VerticalAlignment="Top" FontSize="18" Click="CreerLivret"/>
    </Grid>

</PageFunction>
